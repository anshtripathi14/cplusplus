#include<iostream>
using namespace std;
int main()
{
    int n;
    cout<<"Enter the year\n";
    cin>>n;
    if((n%4==0)&&(n%100!=0))
    {
        cout<<"This is a leap year";
    }
    else
    if(n%100==0)
    {
        cout<<"This year is not a leap year";
    }
    else
    if(n%400==0)
    {
        cout<<"This is a leap year";
    }
    else
    {
        cout<<"This year is not a leap year";
    }
}
