#include<iostream>
using namespace std;
int main()
{
    int n1,n2;
    cout<<"Enter two numbers\n";
    cin>>n1>>n2;
    cout<<"\nSum is "<<n1+n2;
    cout<<"\nDifference is "<<n1-n2;
    cout<<"\nProduct is "<<n1*n2;
    cout<<"\nDivision is "<<n1/n2;
    cout<<"\nModulus is "<<n1%n2;
    return 0;
}
